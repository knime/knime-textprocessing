            name="org.knime.ext.textprocessing.language.german.nodes.tagging.stanfordnlpnetagger.nermodels.GermanHgcModel"
            type="Named Entity Recognition">
      </StanfordTaggerModel>
   </extension>
   <extension
         point="org.knime.ext.textprocessing.TokenizerFactory">
      <TokenizerFactory
            TokenizerFactory="org.knime.ext.textprocessing.language.german.nodes.tokenization.tokenizer.word.OpenNlpGermanWordTokenizerFactory">
      </TokenizerFactory>
   </extension>

</plugin>
